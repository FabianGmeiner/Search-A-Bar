<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane
        xmlns:fx="http://javafx.com/fxml/1"
        fx:controller="DialogPasswordController"
        maxHeight="-Infinity"
        maxWidth="-Infinity"
        minHeight="-Infinity"
        minWidth="-Infinity"
        prefHeight="220.0"
        prefWidth="400.0"
        xmlns="http://javafx.com/javafx/8">
    <children>
        <GridPane
                layoutX="72.0"
                layoutY="27.0"
                prefHeight="168.0"
                prefWidth="286.0"
                AnchorPane.bottomAnchor="0.0"
                AnchorPane.leftAnchor="0.0"
                AnchorPane.rightAnchor="0.0"
                AnchorPane.topAnchor="0.0">
            <columnConstraints>
                <ColumnConstraints
                        hgrow="SOMETIMES"
                        maxWidth="195.0"
                        minWidth="10.0"
                        prefWidth="144.0"/>
                <ColumnConstraints
                        hgrow="SOMETIMES"
                        maxWidth="276.0"
                        minWidth="10.0"
                        prefWidth="256.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints
                        minHeight="10.0"
                        prefHeight="30.0"
                        vgrow="SOMETIMES"/>
                <RowConstraints
                        minHeight="10.0"
                        prefHeight="30.0"
                        vgrow="SOMETIMES"/>
                <RowConstraints
                        minHeight="10.0"
                        prefHeight="30.0"
                        vgrow="SOMETIMES"/>
                <RowConstraints
                        minHeight="10.0"
                        prefHeight="30.0"
                        valignment="TOP"
                        vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <PasswordField
                        fx:id="mDialogPasswordOld"
                        prefHeight="30.0"
                        promptText="Aktuelles Passwort eingeben"
                        GridPane.columnIndex="1"/>
                <PasswordField
                        fx:id="mDialogPasswordNew"
                        prefHeight="30.0"
                        promptText="Neues Passwort wählen"
                        GridPane.columnIndex="1"
                        GridPane.rowIndex="1"/>
                <PasswordField
                        fx:id="mDialogPasswordConfirm"
                        prefHeight="30.0"
                        promptText="Neues Passwort bestätigen"
                        GridPane.columnIndex="1"
                        GridPane.rowIndex="2"/>
                <Label
                        text="Altes Passwort"/>
                <Label
                        text="Neues Passwort"
                        GridPane.rowIndex="1"/>
                <Label
                        text="Passwort bestätigen"
                        GridPane.rowIndex="2"/>
                <HBox
                        alignment="BOTTOM_RIGHT"
                        prefHeight="100.0"
                        prefWidth="200.0"
                        GridPane.columnIndex="1"
                        GridPane.rowIndex="3">
                    <children>
                        <Button
                                fx:id="mDialogPasswordCancel"
                                onAction="#handleButtonCancel"
                                mnemonicParsing="false"
                                prefHeight="30.0"
                                prefWidth="100.0"
                                text="Abbruch"/>
                        <Button fx:id="mDialogPasswordOK"
                                onAction="#handleButtonOK"
                                defaultButton="true"
                                mnemonicParsing="false"
                                prefHeight="30.0"
                                prefWidth="100.0"
                                text="Speichern">
                            <HBox.margin>
                                <Insets
                                        left="10.0"/>
                            </HBox.margin>
                        </Button>
                    </children>
                    <GridPane.margin>
                        <Insets
                                bottom="10.0"
                                right="10.0"/>
                    </GridPane.margin>
                </HBox>
            </children>
        </GridPane>
    </children>
    <padding>
        <Insets
                bottom="10.0"
                left="10.0"
                right="10.0"
                top="10.0"/>
    </padding>
</AnchorPane>
